<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/large_margin"
    android:columnCount="8"
    android:rowCount="2"
    android:visibility="gone">


    <ImageButton
        android:id="@+id/draw_shape_line"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="0"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_line" />

    <ImageButton
        android:id="@+id/draw_shape_line_dash"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="0"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_line_dash" />

    <ImageButton
        android:id="@+id/draw_shape_square"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="1"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_square" />

    <ImageButton
        android:id="@+id/draw_shape_square_solid"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="1"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_square_solid" />

    <ImageButton
        android:id="@+id/draw_shape_round"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="2"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_round" />

    <ImageButton
        android:id="@+id/draw_shape_round_solid"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="2"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_round_solid" />

    <ImageButton
        android:id="@+id/draw_shape_luna1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="3"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_luna1" />

    <ImageButton
        android:id="@+id/draw_shape_luna2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="3"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_luna2" />

    <ImageButton
        android:id="@+id/draw_shape_up_arrow1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="4"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_up_arrow1" />

    <ImageButton
        android:id="@+id/draw_shape_up_arrow2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="4"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_up_arrow2" />

    <ImageButton
        android:id="@+id/draw_shape_up_arrow3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="5"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_up_arrow3" />

    <ImageButton
        android:id="@+id/draw_shape_up_arrow4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="5"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_up_arrow4" />

    <ImageButton
        android:id="@+id/draw_shape_right_arrow1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="6"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_right_arrow1" />

    <ImageButton
        android:id="@+id/draw_shape_right_arrow2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="6"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_right_arrow2" />

    <ImageButton
        android:id="@+id/draw_shape_right_arrow3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="7"
        android:layout_row="0"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_right_arrow3" />

    <ImageButton
        android:id="@+id/draw_shape_down_arrow"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_column="7"
        android:layout_row="1"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        android:scaleType="fitCenter"
        android:layout_margin="@dimen/tiny_margin"
        android:background="@drawable/ripple_bg_nolimit_trans_small"
        android:tint="@color/colorLightGray"
        android:src="@mipmap/shape_down_arrow" />

</GridLayout>